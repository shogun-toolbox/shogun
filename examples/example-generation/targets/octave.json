{
    "Program": "modshogun\n\n$program",
    "Statement": "$statement;\n",
    "Comment": "%$comment\n",
    "Init": {
        "Construct": "$name = $type($arguments)",
        "Copy": "$name = $expr"
    },
    "Assign": "$name = $expr",
    "Type": {
        "BasicType": "$type",
        "ObjectType": "$type"
    },
    "Expr": {
        "StringLiteral": "'$literal'",
        "BoolLiteral": {
            "True": "true",
            "False": "false"
        },
        "NumberLiteral": "$number",
        "MethodCall": "$object.$method($arguments)",
        "Identifier": "$identifier"
    },
    "Print": "disp($expr)"
}