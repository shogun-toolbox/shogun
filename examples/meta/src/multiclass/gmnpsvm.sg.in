File f_feats_train = read_csv("@SHOGUN_DATA@/fm_train_real.dat")
File f_feats_test = read_csv("@SHOGUN_DATA@/fm_test_real.dat")
File f_labels_train = read_csv("@SHOGUN_DATA@/label_train_multiclass.dat")

#![create_features]
Features feats_train = create_features(f_feats_train)
Features feats_test = create_features(f_feats_test)
Labels labels_train = create_labels(f_labels_train)
#![create_features]

#![create_machine]
Kernel gaussian_kernel = create_kernel("GaussianKernel", log_width=2.1)
Machine gmnpsvm = create_machine("GMNPSVM", C=1.0, kernel=gaussian_kernel, labels=labels_train)
#![create_machine]

#![train_and_apply]
gmnpsvm.train(feats_train)
Labels test_labels = gmnpsvm.apply(feats_test)
RealVector test_labels_vector = test_labels.get_real_vector("labels")
#![train_and_apply]
