File traindat = read_csv("@SHOGUN_DATA@/fm_train_real.dat")
File testdat = read_csv("@SHOGUN_DATA@/fm_test_real.dat")

#![create_features]
Features feats_train = create_features(traindat)
Features feats_test = create_features(testdat)
#![create_features]

#![create_kernel]
Kernel gaussian_kernel = create_kernel("GaussianKernel", log_width=2.0)
gaussian_kernel.init(feats_train, feats_train)
#![create_kernel]

#![get_kernel_features]
RealMatrix K_train_train = gaussian_kernel.get_kernel_matrix()
gaussian_kernel.init(feats_train, feats_test)
RealMatrix K_train_test = gaussian_kernel.get_kernel_matrix()
